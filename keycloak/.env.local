# Keycloak Environment Configuration - Local Development

# Keycloak Version
KEYCLOAK_VERSION=26.0.4

# Admin Credentials
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Database Configuration
KC_DB_NAME=keycloak
KC_DB_USERNAME=temporal
KC_DB_PASSWORD=temporal

# Network and Port Configuration
KEYCLOAK_PORT=18080

# PostgreSQL Version (should match your postgresql service)
POSTGRESQL_VERSION=16

# Resource Limits
KEYCLOAK_MEMORY_LIMIT=1G
KEYCLOAK_MEMORY_RESERVATION=512M

# Logging Configuration
LOGGING_DRIVER=local
LOG_MAX_SIZE=50m
LOG_MAX_FILE=2

# Configuration complete 