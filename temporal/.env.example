# Temporal Environment Configuration
# This file contains the environment variables used by Temporal services
# Copy this file to .env.local - secrets will be auto-generated

# =============================================================================
# Temporal Versions
# =============================================================================
# Specify the Temporal Docker image versions to use
TEMPORAL_VERSION=1.28.0
TEMPORAL_UI_VERSION=2.36.0

# =============================================================================
# Host IP
# =============================================================================
AUTH_HOST=
TEMPORAL_HOST=

# =============================================================================
# Database Configuration
# =============================================================================
POSTGRES_USER=dbuser
POSTGRES_PASSWORD=

# =============================================================================
# Temporal UI Authentication
# =============================================================================
# Client secret for Temporal UI authentication with Keycloak
TEMPORAL_UI_CLIENT_SECRET=
