# XiansAi UI Environment Configuration
# Copy this file to .env and configure for your environment

# =============================================================================
# API Configuration
# =============================================================================
# Base URL for the XiansAi Server API
REACT_APP_API_URL=http://localhost:5000

# =============================================================================
# Authentication Configuration
# =============================================================================
# Authentication provider: 'auth0' or 'entraId'
REACT_APP_AUTH_PROVIDER=auth0

# Auth0 Configuration (when REACT_APP_AUTH_PROVIDER=auth0)
REACT_APP_AUTH0_DOMAIN=your-auth0-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your-auth0-client-id
REACT_APP_AUTH0_AUDIENCE=https://your-api-audience
REACT_APP_AUTH0_ORGANIZATION_CLAIM=https://xians.ai/tenants

# Entra ID Configuration (when REACT_APP_AUTH_PROVIDER=entraId)
# REACT_APP_ENTRA_ID_CLIENT_ID=your-entra-id-client-id
# REACT_APP_ENTRA_ID_AUTHORITY=https://login.microsoftonline.com/your-tenant-id
# REACT_APP_ENTRA_ID_SCOPES=User.Read,openid,profile
# REACT_APP_ENTRA_ID_ORGANIZATION_CLAIM=https://login-dev.parkly.no/tenants
# REACT_APP_ENTRA_ID_KNOWN_AUTHORITIES=https://login.microsoftonline.com/your-tenant-id

# =============================================================================
# Module Configuration
# =============================================================================
# Enable/disable application modules (set to 'false' to disable)
REACT_APP_ENABLE_PUBLIC_MODULE=true
REACT_APP_ENABLE_MANAGER_MODULE=true
